// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Birth {
  id      String @id @default(cuid()) @map("_id")
  owner   String
  BummyId Int
  momId   Int
  dadId   Int
  genes   Int
}

model Exhaust {
  id    String @id @default(cuid()) @map("_id")
  owner String
  momId Int
  dadId Int
}

model Transfer {
  id      String @id @default(cuid()) @map("_id")
  from    String
  to      String
  tokenId Int
}
