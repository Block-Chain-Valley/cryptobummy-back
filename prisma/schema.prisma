// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Birth {
  id          String   @id @default(cuid()) @map("_id")
  blockNumber Int
  createdAt   DateTime @default(now())

  owner   String
  BummyId String
  momId   String
  dadId   String
  genes   String
}

model Exhaust {
  id    String @id @default(cuid()) @map("_id")
  owner String
  momId Int
  dadId Int
}

model Transfer {
  id      String @id @default(cuid()) @map("_id")
  from    String
  to      String
  tokenId Int
}
